    <DataTemplate DataType="{x:Type vm:ObjectsVM}">
        <view:ObjectsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OutputInfoesVM}">
        <view:OutputInfoesView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OutputsVM}">
        <view:OutputsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UserRolesVM}">
        <view:UserRolesView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UsersVM}">
        <view:UsersView />
    </DataTemplate>
</ResourceDictionary>